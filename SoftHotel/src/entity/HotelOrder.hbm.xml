<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC
        "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
        "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">
<hibernate-mapping package="entity">
	<class name="HotelOrder" table="hotel_order">
		<id name="oid">
			<generator class="increment"></generator>
		</id>
		<property name="orderName"></property>
		<property name="phone"></property>
		<property name="ltime"></property>
		<property name="massage"></property>
		<property name="payment"></property>
		<property name="orderTime"></property>
		<property name="price"></property>
		<property name="status"></property>
		<many-to-one name="uid" lazy="false"></many-to-one>
		<property name="orderNum"></property>
	</class>
</hibernate-mapping>

